{
  "name": "partnersapi",
  "version": "1.0.0",
  "description": "Azure Functions for Legal Documents API",
  "main": "dist/functions/*.js",
  "scripts": {
    "build": "tsc",
    "copy-shared": "cp -r ../shared ./dist/ && cp ../shared/package.json ./dist/shared/",
    "postinstall": "cd ../shared && yarn install --no-engines",
    "watch": "tsc -w",
    "prestart": "yarn build",
    "start": "func start",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "deploy": "yarn build && func azure functionapp publish bus-to-partners-api",
    "dev": "yarn build && yarn start"
  },
  "dependencies": {
    "@partnersapi/shared": "file:../shared",
    "@azure/functions": "^4.5.0",
    "@azure/service-bus": "^7.9.4",
    "@azure/storage-blob": "^12.17.0",
    "axios": "^1.6.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "mssql": "^10.0.1",
    "multiparty": "^4.2.3",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/multiparty": "^0.0.36",
    "@types/node": "^20.19.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/mssql": "^9.1.7",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "azure-functions",
    "typescript",
    "legal-documents",
    "service-bus",
    "blob-storage",
    "partners-api"
  ],
  "author": "Milestone Software Team",
  "license": "MIT"
}
