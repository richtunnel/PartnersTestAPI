{
  "name": "demographics-api-express",
  "version": "1.0.0",
  "description": "Demographics API with Express, Service Bus, and Queue Processing",
  "main": "dist/app-service/server.js",
  "scripts": {
 "prebuild": "cd ../shared && yarn install",
    "build": "tsc && yarn copy-shared",
    "copy-shared": "cp -r ../shared ./dist/ && cd dist/shared && yarn install --production",
    "start": "node dist/app-service/server.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/app-service/server.ts",
    "worker": "NODE_ENV=development node dist/azure-functions/queue-triggers/documentProcessorTrigger.js",
    "webhook-processor": "node dist/azure-functions/workers/webhookProcessor.js"
  },
  "dependencies": {
    "@partnersapi/shared": "file:../shared",
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/functions": "^4.7.2-preview",
    "@azure/identity": "^4.11.1",
    "@azure/monitor-query": "^1.3.3",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.28.0",
    "@azure/storage-queue": "^12.27.0",
    "axios": "^1.6.2",
    "azurite": "^3.35.0",
    "compression": "^1.7.4",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "cypress": "^15.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^11.7.1",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "newman": "^6.2.1",
    "node-cron": "^4.2.1",
    "os": "^0.1.2",
    "prom-client": "^15.1.3",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "zlib": "^1.0.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@types/cypress": "^1.1.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/mssql": "^9.1.7",
    "@types/node-cron": "^3.0.11",
    "@types/sinon": "^17.0.4",
    "@types/winston": "^2.4.4",
    "artillery": "^2.0.4",
    "jest": "^30.1.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests/"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
